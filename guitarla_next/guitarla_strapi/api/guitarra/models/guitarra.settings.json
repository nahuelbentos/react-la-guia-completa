{
  "kind": "collectionType",
  "collectionName": "guitarras",
  "info": {
    "name": "Guitarra"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "richtext",
      "required": true
    },
    "imagen": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "precio": {
      "type": "integer"
    },
    "url": {
      "type": "uid",
      "targetField": "nombre"
    }
  }
}
